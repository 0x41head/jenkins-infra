apiVersion: v1
kind: Secret 
metadata: 
    name: ldap
    namespace: ldap
type: Opaque
data:
    azurestorageaccountname: <%= @parameters['storage_account_name']%>
    azurestorageaccountkey: <%= @parameters['storage_account_key'] %>
    ldap.admin.password: <%= @parameters['ldap_admin_password'] %>
    cert.pem: <%= @parameters['ldap_tls_crt'] %>
    privkey.key: <%= @parameters['ldap_tls_key'] %>
    cacert.pem: <%= @parameters['ca_tls_crt'] %>

