apiVersion: v1
kind: Secret
metadata:
  name: jenkinsadmin
  namespace: chatbot
data:
  .github: |
    login=<%= @parameters['github_login'] %>
    password=<%= @parameters['github_password'] %>

  .jenkins-ci.org: |
    userName=<%= @parameters['jira_login'] %>
    password=<%= @parameters['jira_password'] %>
