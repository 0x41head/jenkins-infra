apiVersion: v1
kind: Secret 
metadata: 
    name: accountapp
type: Opaque
data:
    ldap.password: <%= @parameters['ldap_password'] %>
    jira.password: <%= @parameters['jira_password'] %>
    smtp.password: <%= @parameters['smtp_password'] %>
    recaptcha.key.private: <%= @parameters['recaptcha_private_key'] %>
    azurestorageaccountname: <%= @parameters['storage_account_name']%>
    azurestorageaccountkey: <%= @parameters['storage_account_key'] %>
